{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.associations": {
    "math": "cpp",
    "*.inc": "cpp",
    "optional": "cpp",
    "cmath": "cpp",
    "complex": "cpp",
    "cstdlib": "cpp",
    "chrono": "cpp"
  }
}
